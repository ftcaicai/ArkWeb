<% if (t != null) {%>
<table class="ui right aligned celled striped table">
    <thead>
    <tr>
        <th class="left aligned">玩家</th>
        <th>金额(RMB)</th>
        <th>时间</th>
    </tr>
    </thead>
    <tbody>
    <%  t.forEach(function(e){ %>
    <tr>
        <td class="left aligned"><%= e.charsid %></td>
        <td><%= e.level %></td>
        <td><%= e.class %></td>
    </tr>
    <% }) %>
    </tbody>
    <tfoot>
    <tr><th colspan="3">
      <%- include("base/footer", {foot:{page:foot.page,count:foot.count, url:'/users/'}})%>
    </th>
  </tr></tfoot>
</table>
<% } else { %>
<div class="ui message">
    <div class="header">查无数据 </div>
    <p>We just updated our privacy policy here to better service our customers. We recommend reviewing the changes.</p>
</div>
<%} %>