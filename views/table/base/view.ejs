<thead>
<tr>
    <% table.kv.forEach(function (kv) { %>
        <th
                <% if (kv.class != null){ %>
                    class="<%= kv.class %>"
                <%}%>
        >
            <% if (kv.headtag){ %>
                <%- kv.headtag %>
            <%}%>

            <%= kv.text %>
        </th>
    <%})%>
</tr>
</thead>
<tbody>
<% if  (locals.t) {%>
<%  t.forEach(function(e){ %>
<tr>
    <% table.kv.forEach(function (kv) { %>

        <td
                <% if (kv.class != null){ %>
                class="<%= kv.class %>"
                <%}%>
        >
            <% if (kv.type && kv.type == "time") {%>
                <%= e[kv.dbkey].toLocaleString() %>
            <%}else{%>
                <%= e[kv.dbkey] %>
            <%}%>
        </td>

    <%})%>
</tr>
<% }) %>

<%} else {%>
<tr>
    <td colspan="<%=table.kv.length%>">
        <div class="ui message">
            <div class="header">查无数据</div>
        </div>
    </td>
</tr>
<%}%>
</tbody>
<tfoot>

<% if(locals.t){%>

<tr >
    <% table.kv.forEach(function(kv){ %>
    <th class="no-sort">
        <% if(kv.filter) {%>
        <div class="ui label">
            $=西瓜
            <i class="delete icon"></i>
        </div>
        <%}else{%>
        &nbsp;
        <%}%>
    </th>
    <% }) %>
</tr>
<%}%>

<tr>
<th colspan="<%=table.kv.length%>">
    <%- include("footer")%>
</th>
</tr>
</tfoot>